{
  "name": "@orbits-ui/react",
  "version": "0.0.11",
  "description": "A React Component Library made with Tailwind CSS",
  "repository": "git@github.com:avneesh0612/Orbits-UI.git",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "dev": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "rollup -c",
    "format": "prettier --write \"{src,styles}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "format:check": "prettier --check \"{src,styles}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "lint": "eslint --fix '{src,styles}/**/*.{js,jsx,ts,tsx,json,yaml,md}'",
    "lint:check": "eslint '{src,styles}/**/*.{js,jsx,ts,tsx,json,yaml,md}'",
    "prepare": "husky install",
    "pre-commit": "yarn run format && yarn run lint && git add -A .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "chromatic": "npx chromatic --project-token=5ba96cb81b5e"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.4.17",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/addon-links": "^6.4.17",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.17",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.4.2",
    "commitlint": "^16.1.0",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "postcss": "^8",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.5",
   "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
