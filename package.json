{
  "name": "@orbits-ui/react",
  "version": "0.0.11",
  "description": "A React Component Library made with Tailwind CSS",
  "repository": "git@github.com:avneesh0612/Orbits-UI.git",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "dev": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "rollup -c",
    "format": "prettier --write \"{src,styles}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "format:check": "prettier --check \"{src,styles}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "prepare": "husky install",
    "pre-commit": "yarn run format && yarn build && git add -A .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "chromatic": "npx chromatic --project-token=5ba96cb81b5e"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.9",
    "@types/react": "^17.0.37",
    "autoprefixer": "^10",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.2.0",
    "husky": "^7.0.4",
    "postcss": "^8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "standard-version": "^9.3.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {}
}
